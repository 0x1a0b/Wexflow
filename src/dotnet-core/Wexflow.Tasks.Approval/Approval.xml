<?xml version="1.0" encoding="utf-8" ?>
<Tasks>
  <!--
    Approval is a sequential task that marks the current workflow as needing approval.
    To use this task the workflow needs to be an approval workflow. This can be done 
    whether from the designer or by editing the XML of the workflow.
  -->
  <Task id="$int" name="Approval" description="$string" enabled="true|false">
  </Task>
</Tasks>
